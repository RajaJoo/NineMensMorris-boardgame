<Window x:Class="Mylly.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mylly"
        x:Name="ikkuna"
        mc:Ignorable="d"
        Title="Settings" Height="300" Width="300">
    <Grid>
        <Button Content="OK" HorizontalAlignment="Left" Margin="41,219,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1" IsDefault="True"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="174,219,0,0" VerticalAlignment="Top" Width="75" IsCancel="True"/>
        <Button x:Name="button" Content="Background Color" HorizontalAlignment="Left" Margin="77,32,0,0" VerticalAlignment="Top" Width="123" Click="button_Click"/>
        <Button x:Name="button1" Content="Game Piece Color" HorizontalAlignment="Left" Margin="77,78,0,0" VerticalAlignment="Top" Width="123" Click="button1_Click"/>
        <Ellipse x:Name="ellipse1" Fill="{Binding ElementName=ikkuna, Path=Laudanvari}" HorizontalAlignment="Left" Height="20" Margin="205,32,0,0" Stroke="Black" VerticalAlignment="Top" Width="20"/>
        <Ellipse Fill="{Binding ElementName=ikkuna, Path=Nappulanvari}" HorizontalAlignment="Left" Height="20" Margin="205,78,0,0" Stroke="Black" VerticalAlignment="Top" Width="20"/>
    </Grid>
</Window>
